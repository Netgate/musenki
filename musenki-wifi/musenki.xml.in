<clixon-config xmlns="http://clicon.org/config">
  <CLICON_CONFIGFILE>@SYSCONFDIR@/clixon/musenki.xml</CLICON_CONFIGFILE>
  <CLICON_CONFIGDIR>@SYSCONFDIR@/clixon/musenki</CLICON_CONFIGDIR>
  <CLICON_FEATURE>ietf-netconf:startup</CLICON_FEATURE>
  <CLICON_FEATURE>clixon-restconf:allow-auth-none</CLICON_FEATURE>
  <CLICON_YANG_DIR>@DATADIR@/clixon</CLICON_YANG_DIR>
  <CLICON_YANG_DIR>@DATADIR@/openconfig/public/release/models</CLICON_YANG_DIR>
  <CLICON_YANG_MODULE_MAIN>musenki-wifi</CLICON_YANG_MODULE_MAIN>
  <CLICON_CLI_MODE>musenki</CLICON_CLI_MODE>
  <CLICON_CLI_LINESCROLLING>0</CLICON_CLI_LINESCROLLING>
  <CLICON_CLI_TAB_MODE>0</CLICON_CLI_TAB_MODE>
  <CLICON_CLISPEC_DIR>@LIBEXECDIR@/musenki/clispec</CLICON_CLISPEC_DIR>
  <CLICON_BACKEND_DIR>@LIBEXECDIR@/musenki/backend</CLICON_BACKEND_DIR>
  <CLICON_SOCK>@LOCALSTATEDIR@/musenki.sock</CLICON_SOCK>
  <CLICON_BACKEND_PIDFILE>@LOCALSTATEDIR@/musenki.pidfile</CLICON_BACKEND_PIDFILE>
  <CLICON_BACKEND_RESTCONF_PROCESS>true</CLICON_BACKEND_RESTCONF_PROCESS>
  <CLICON_XMLDB_DIR>@LOCALSTATEDIR@/musenki</CLICON_XMLDB_DIR>
  <CLICON_STARTUP_MODE>startup</CLICON_STARTUP_MODE>
  <CLICON_SOCK_GROUP>clicon</CLICON_SOCK_GROUP>
  <CLICON_RESTCONF_USER>clicon</CLICON_RESTCONF_USER>
  <CLICON_RESTCONF_PRIVILEGES>drop_perm</CLICON_RESTCONF_PRIVILEGES>
  <CLICON_RESTCONF_INSTALLDIR>@SBINDIR@</CLICON_RESTCONF_INSTALLDIR>
</clixon-config>
